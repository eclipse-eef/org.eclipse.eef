<plugin>

   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <factory
            uri="http://www.eclipse.org/emf/eef/editingmodel/1.0.0"
            class="org.eclipse.emf.eef.runtime.editingModel.provider.EditingModelItemProviderAdapterFactory"
            supportedTypes=
              "org.eclipse.emf.edit.provider.IEditingDomainItemProvider
               org.eclipse.emf.edit.provider.IStructuredItemContentProvider
               org.eclipse.emf.edit.provider.ITreeItemContentProvider
               org.eclipse.emf.edit.provider.IItemLabelProvider
               org.eclipse.emf.edit.provider.IItemPropertySource"/>
   </extension>

   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <factory
            uri="http://www.eclipse.org/emf/eef/query/1.0"
            class="org.eclipse.emf.eef.runtime.query.provider.QueryItemProviderAdapterFactory"
            supportedTypes=
              "org.eclipse.emf.edit.provider.IEditingDomainItemProvider
               org.eclipse.emf.edit.provider.IStructuredItemContentProvider
               org.eclipse.emf.edit.provider.ITreeItemContentProvider
               org.eclipse.emf.edit.provider.IItemLabelProvider
               org.eclipse.emf.edit.provider.IItemPropertySource"/>
   </extension>

   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <factory
            uri="http://www.eclipse.org/emf/eef/views/1.0.0"
            class="org.eclipse.emf.eef.views.provider.ViewsItemProviderAdapterFactory"
            supportedTypes=
              "org.eclipse.emf.edit.provider.IEditingDomainItemProvider
               org.eclipse.emf.edit.provider.IStructuredItemContentProvider
               org.eclipse.emf.edit.provider.ITreeItemContentProvider
               org.eclipse.emf.edit.provider.IItemLabelProvider
               org.eclipse.emf.edit.provider.IItemPropertySource"/>
   </extension>

    <extension point="org.eclipse.emf.ecore.extension_parser">
      <parser
            class="org.eclipse.emf.eef.runtime.editingModel.resources.EditingModelResourceFactory"
            type="editingmodel"/>
    </extension>

   <extension
         point="org.eclipse.emf.ecore.uri_mapping">
      <mapping
            source="eeftoolkit://EEFExtended.toolkit"
            target="eeftoolkit:/org.eclipse.emf.eef.editor/org.eclipse.emf.eef.editor.internal.propertyeditors.extended.EEFExtendedToolkit">
      </mapping>
   </extension>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.eclipse.emf.eef.runtime.editingModel.presentation.EditingModelModelWizardID"
            name="%_UI_EditingModelModelWizard_label"
            class="org.eclipse.emf.eef.runtime.editingModel.presentation.EditingModelModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/EditingModelModelFile.gif">
         <description>%_UI_EditingModelModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="org.eclipse.emf.eef.runtime.editingModel.presentation.EditingModelEditorID"
            name="%_UI_EditingModelEditor_label"
            icon="icons/full/obj16/EditingModelModelFile.gif"
            extensions="editingmodel,views"
            class="org.eclipse.emf.eef.editor.EEFReflectiveEditor"
            contributorClass="org.eclipse.emf.eef.runtime.editingModel.presentation.EditingModelActionBarContributor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="org.eclipse.emf.eef.editor.category"
            class="org.eclipse.emf.eef.view.EEFReflectiveView"
            icon="icons/overview_obj.gif"
            id="org.eclipse.emf.eef.view.EEFReflexiveView"
            name="EEF Reflective View"
            restorable="true">
      </view>
      <category
            id="org.eclipse.emf.eef.editor.category"
            name="EEF">
      </category>
   </extension>

</plugin>
